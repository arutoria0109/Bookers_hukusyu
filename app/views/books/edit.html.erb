<h2>Editing Book</h2>
<%= form_with model: @book, url: book_path do |f| %>
<%= f.label "Title" %></br>
<%= f.text_field :title %></br>
<%= f.label "Body" %></br>
<%= f.text_area :body %></br>
<%= f.submit "Update Book" %>
<% end %>

<%= link_to "Show", book_path %>|
<%= link_to "Back", books_path %>
